<rdf:RDF xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:dct="http://purl.org/dc/terms/" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <owl:Ontology>
    <rdfs:label>NLM Dataset Metadata Model</rdfs:label>
    <skos:definition>DATMM is an RDF model created by the National Library of Medicine (NLM) for collecting data about biomedical datasets published in a multitiude of sites across the Web.Â  The intent is to use DATMM to facilitate finding and linking to those biomedical datasets from a single website provided by NLM.</skos:definition>
    <dct:creator>http://id.loc.gov/vocabulary/organizations/dnlm</dct:creator>
    <owl:versionInfo>3.0.0</owl:versionInfo>
    <dct:created>2024-12-18</dct:created>
    <dct:hasVersion>3.0.0</dct:hasVersion>
    <dct:modified>2024-12-18</dct:modified>
    <rdfs:comment>In the DATMM scheme owl:versionInfo and dct:created always reflect the most recent full version X.0.0, while dct:hasVersion followed by dct:modified reference the most recent minor revision within that full version.</rdfs:comment>
  </owl:Ontology>
  <!--#######################################-->
  <!--##===================================##-->
  <!--##==           CLASSES             ==##-->
  <!--##===================================##-->
  <!--#######################################-->
  <owl:Class rdf:about="http://id.nlm.nih.gov/datmm/Dataset">
    <rdfs:label>Dataset</rdfs:label>
    <dct:identifier>datmm:Dataset</dct:identifier>
    <skos:definition>A discrete collection of data gathered for use in research</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent">
    <rdfs:label>Agent</rdfs:label>
    <dct:identifier>foaf:Agent</dct:identifier>
    <skos:definition>An agent (eg. person, group, software or physical artifact); Things that do stuff</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/2004/02/skos/core#Concept">
    <rdfs:label>Concept</rdfs:label>
    <dct:identifier>skos:Concept</dct:identifier>
    <skos:definition>An idea or notion; a unit of thought</skos:definition>
    <dct:modified xml:lang="en">2024-04-04 adjusted definition</dct:modified>
  </owl:Class>
  <owl:Class rdf:about="http://schema.org/Grant">
    <rdfs:label>Grant</rdfs:label>
    <dct:identifier>schema:Grant</dct:identifier>
    <skos:definition>A grant, typically financial or otherwise quantifiable, of resources.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://schema.org/Intangible"/>
    <dct:modified xml:lang="en">2024-12-18 Added</dct:modified>
  </owl:Class>
  <owl:Class rdf:about="http://id.loc.gov/ontologies/bibframe/Contribution">
    <rdfs:label>Contribution</rdfs:label>
    <dct:identifier>bf:Contribution</dct:identifier>
    <skos:definition>Agent and role with respect to the resource being described</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/dc/dcmitype/Collection">
    <rdfs:label>Collection</rdfs:label>
    <dct:identifier>dcmitype:Collection</dct:identifier>
    <skos:definition>An aggregation of resources</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="http://id.nlm.nih.gov/datmm/Repository">
    <rdfs:label>Repository</rdfs:label>
    <dct:identifier>datmm:Repository</dct:identifier>
    <skos:definition>A network storage resource from which datasets are accessible</skos:definition>
  </owl:Class>
  <owl:Class rdf:about="http://id.nlm.nih.gov/datmm/Documentation">
    <rdfs:label>Documentation</rdfs:label>
    <dct:identifier>datmm:Documentation</dct:identifier>
    <skos:definition>Contextual documentation about the Dataset, e.g., articles, research study, grant application, clinical trial, research instruments (CDEs), et al.</skos:definition>
    <dct:modified xml:lang="en">2024-04-04 adjusted definition</dct:modified>
  </owl:Class>
  <!--###############################################-->
  <!--##=========================================##-->
  <!--##==           DATATYPE PROPERTIES       ==##-->
  <!--##=========================================##-->
  <!--###############################################-->
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier">
    <rdfs:label xml:lang="en">identifier</rdfs:label>
    <dct:identifier>dct:identifier</dct:identifier>
    <skos:definition xml:lang="en">An unambiguous reference to the resource within a given context.</skos:definition>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Documentation"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Repository"/>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/description">
    <rdfs:label xml:lang="en">description</rdfs:label>
    <dct:identifier>dct:description</dct:identifier>
    <skos:definition xml:lang="en">An account of the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://schema.org/name">
    <rdfs:label xml:lang="en">name</rdfs:label>
    <dct:identifier>schema:name</dct:identifier>
    <skos:definition xml:lang="en">The name of the item.</skos:definition>
    <rdfs:domain rdf:resource="http://schema.org/Grant"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title">
    <rdfs:label xml:lang="en">title</rdfs:label>
    <dct:identifier>dct:title</dct:identifier>
    <skos:definition xml:lang="en">A name given to the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Documentation"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Repository"/>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://schema.org/identifier">
    <rdfs:label xml:lang="en">identifier</rdfs:label>
    <dct:identifier>schema:identifier</dct:identifier>
    <skos:definition xml:lang="en">The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc. </skos:definition>
    <rdfs:domain rdf:resource="http://schema.org/Grant"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#type">
    <rdfs:label xml:lang="en">type</rdfs:label>
    <dct:identifier>rdf:type</dct:identifier>
    <skos:definition xml:lang="en">The subject is an instance of a class.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Documentation"/>
    <dct:modified xml:lang="en">2024-04-04 adjusted definition to match RDF scheme</dct:modified>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/language">
    <rdfs:label xml:lang="en">language</rdfs:label>
    <dct:identifier>dct:language</dct:identifier>
    <skos:definition xml:lang="en">A language of the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/homepage">
    <rdfs:label xml:lang="en">homepage</rdfs:label>
    <dct:identifier>foaf:homepage</dct:identifier>
    <skos:definition xml:lang="en">A homepage for some thing.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Documentation"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Repository"/>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
    <rdfs:label xml:lang="en">label</rdfs:label>
    <dct:identifier>rdfs:label</dct:identifier>
    <skos:definition xml:lang="en">A human-readable name for the subject.</skos:definition>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:modified xml:lang="en">2024-04-04 adjusted definition to match RDF scheme</dct:modified>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/issued">
    <rdfs:label xml:lang="en">issued</rdfs:label>
    <dct:identifier>dct:issued</dct:identifier>
    <skos:definition xml:lang="en">Date of formal issuance of the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/alternative">
    <rdfs:label xml:lang="en">alternative</rdfs:label>
    <dct:identifier>dct:alternative</dct:identifier>
    <skos:definition xml:lang="en">An alternative name for the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Repository"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://xmlns.com/foaf/0.1/name">
    <rdfs:label xml:lang="en">name</rdfs:label>
    <dct:identifier>foaf:name</dct:identifier>
    <skos:definition xml:lang="en">The foaf:name of something is a simple textual string.</skos:definition>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://id.loc.gov/ontologies/bibframe/role">
    <rdfs:label xml:lang="en">role</rdfs:label>
    <dct:identifier>bf:role</dct:identifier>
    <skos:definition xml:lang="en">Function provided by a contributor, e.g., author, illustrator, etc.</skos:definition>
    <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Contribution"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/bibliographicCitation">
    <rdfs:label xml:lang="en">bibliographicCitation</rdfs:label>
    <dct:identifier>dct:bibliographicCitation</dct:identifier>
    <skos:definition xml:lang="en">A bibliographic reference for the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Documentation"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/rights">
    <rdfs:label xml:lang="en">rights</rdfs:label>
    <dct:identifier>dct:rights</dct:identifier>
    <skos:definition xml:lang="en">Information about rights held in and over the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#inScheme">
    <rdfs:label xml:lang="en">inScheme</rdfs:label>
    <dct:identifier>skos:inScheme</dct:identifier>
    <skos:definition xml:lang="en">is in scheme</skos:definition>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://schema.org/url">
    <rdfs:label xml:lang="en">url</rdfs:label>
    <dct:identifier>schema:url</dct:identifier>
    <skos:definition xml:lang="en">URL of the item.</skos:definition>
    <rdfs:domain rdf:resource="http://schema.org/Grant"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/source">
    <rdfs:label xml:lang="en">source</rdfs:label>
    <dct:identifier>dct:source</dct:identifier>
    <skos:definition xml:lang="en">A related resource from which the described resource is derived.</skos:definition>
    <rdfs:domain rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:DatatypeProperty> 
  <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/adms#status">
    <rdfs:label xml:lang="en">status</rdfs:label>
    <dct:identifier>adms:status</dct:identifier>
    <skos:definition xml:lang="en">The status of the Asset in the context of a particular workflow process.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Collection"/>
  </owl:DatatypeProperty> 
  <owl:DatatypeProperty rdf:about="https://www.w3.org/TR/vocab-org/#org:memberOf">
    <rdfs:label xml:lang="en">memberOf</rdfs:label>
    <dct:identifier>org:memberOf</dct:identifier>
    <skos:definition xml:lang="en">Indicates that an agent (person or other organization) is a member of the Organization with no indication of the nature of that membership or the role played.</skos:definition>
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </owl:DatatypeProperty> 
 
  <!--##############################################-->
  <!--##========================================##-->
  <!--##==           OBJECT PROPERTIES        ==##-->
  <!--##========================================##-->
  <!--##############################################-->
  <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/relation">
    <rdfs:label xml:lang="en">relation</rdfs:label>
    <dct:identifier>dct:relation</dct:identifier>
    <skos:definition xml:lang="en">A related resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:range rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isVersionOf">
    <rdfs:label xml:lang="en">isVersionOf</rdfs:label>
    <dct:identifier>dct:isVersionOf</dct:identifier>
    <skos:definition xml:lang="en">A related resource of which the described resource is a version, edition, or adaptation.</skos:definition>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:range rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:range rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/agent">
    <rdfs:label xml:lang="en">agent</rdfs:label>
    <dct:identifier>bf:agent</dct:identifier>
    <skos:definition xml:lang="en">Entity associated with a resource or element of description, such as the name of the entity responsible for the content or of the publication, printing, distribution, issue, release or production of a resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.loc.gov/ontologies/bibframe/Contribution"/>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://id.loc.gov/ontologies/bibframe/contribution">
    <rdfs:label xml:lang="en">contribution</rdfs:label>
    <dct:identifier>bf:contribution</dct:identifier>
    <skos:definition xml:lang="en">Agent and its role in relation to the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Contribution"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isPartOf">
    <rdfs:label xml:lang="en">isPartOf</rdfs:label>
    <dct:identifier>dct:isPartOf</dct:identifier>
    <skos:definition xml:lang="en">A related resource in which the described resource is physically or logically included.</skos:definition>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:range rdf:resource="http://id.nlm.nih.gov/datmm/Repository"/>
    <rdfs:range rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://schema.org/funder">
    <rdfs:label xml:lang="en">funder</rdfs:label>
    <dct:identifier>schema:funder</dct:identifier>
    <skos:definition xml:lang="en">A person or organization that supports (sponsors) something through some kind of financial contribution.</skos:definition>
    <rdfs:domain rdf:resource="http://schema.org/Grant"/>
    <rdfs:range rdf:resource="http://id.loc.gov/ontologies/bibframe/Contribution"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/isReferencedBy">
    <rdfs:label xml:lang="en">isReferencedBy</rdfs:label>
    <dct:identifier>dct:isReferencedBy</dct:identifier>
    <skos:definition xml:lang="en">A related resource that references, cites, or otherwise points to the described resource.</skos:definition>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:range rdf:resource="http://id.nlm.nih.gov/datmm/Documentation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/subject">
    <rdfs:label xml:lang="en">subject</rdfs:label>
    <dct:identifier>dct:subject</dct:identifier>
    <skos:definition xml:lang="en">A topic of the resource.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Repository"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/hasPart">
    <rdfs:label xml:lang="en">hasPart</rdfs:label>
    <dct:identifier>dct:hasPart</dct:identifier>
    <skos:definition xml:lang="en">A related resource that is included either physically or logically in the described resource.</skos:definition>
    <rdfs:domain rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
    <rdfs:range rdf:resource="http://purl.org/dc/dcmitype/Collection"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://schema.org/funding">
    <rdfs:label xml:lang="en">funding</rdfs:label>
    <dct:identifier>schema:funding</dct:identifier>
    <skos:definition xml:lang="en">A Grant that directly or indirectly provide funding or sponsorship for this item.</skos:definition>
    <rdfs:domain rdf:resource="http://id.nlm.nih.gov/datmm/Dataset"/>
    <rdfs:range rdf:resource="http://schema.org/Grant"/>
  </owl:ObjectProperty>
</rdf:RDF>
